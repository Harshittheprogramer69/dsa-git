/*P4.4 ProgramÂ· to understand' the use of postfix increment/decrement*/
#include <stdio.h>
#include <conio.h>

void main() {
    int x;
    clrscr();

    x=8;

    printf("x = %d\t",x);
    printf("x = %d\t",x++); //postfix increment
    printf("x = %d\t",x);
    printf("x = %d\t",--x); //postfix decrement
    printf("x = %d\t",x);

    getch();
}